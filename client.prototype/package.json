{
  "name": "archangel-community",
  "version": "1.0.0-beta.1",
  "description": "Archangel Community",
  "main": "dist/bootstrap.js",
  "repository": {
    "type": "git",
    "url": "https://ArchangelCommunity@bitbucket.org/ArchangelCommunity/archangel-community.git"
  },
  "bugs": {
    "type": "git",
    "url": "https://ArchangelCommunity@bitbucket.org/ArchangelCommunity/archangel-community.git/issues"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy": "cpx src/**/*.{html,png,jpg,jpeg,ico,eot,svg,woff,ttf} dist/",
    "scss": "node-sass src/ -o dist/",
    "build": "npm run copy && npm run scss && tsc",
    "copy:watch": "cpx src/**/*.{html,png,jpg,jpeg,ico,eot,svg,woff,ttf} dist/ --watch",
    "scss:watch": "node-sass --watch --recursive src/ -o dist/ ",
    "tsc:watch": "tsc -w",
    "watch": "concurrently \"npm run copy:watch\" \"npm run scss:watch\" \"npm run tsc:watch\"",
    "tslint": "tslint src/**/*.ts",
    "postinstall": "typings install",
    "prepublish": "npm run build",
    "tsc": "tsc",
    "start": "concurrently \"npm run build\" \"npm run watch\" \"npm run serve\"",
    "restart": "concurrently \"npm run watch\" \"npm run serve\"",
    "serve": "http-server -c-1 -o -p 9875 ./",
    "pretest": "npm run build",
    "test": "karma start",
    "posttest": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage.json",
    "posttest1": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "^3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "@angular2-material/button": "2.0.0-alpha.7-4 ",
    "@angular2-material/checkbox": "2.0.0-alpha.7-4 ",
    "@angular2-material/core": "2.0.0-alpha.7-4",
    "@angular2-material/icon": "2.0.0-alpha.7-4 ",
    "@angular2-material/input": "2.0.0-alpha.7-4",
    "@angular2-material/radio": "2.0.0-alpha.7-4",
    "angular2-in-memory-web-api": "0.0.15",
    "angular2-jwt": "0.1.16",
    "core-js": "^2.4.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "0.35.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.31",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@angular/tsc-wrapped": "^0.2.2",
    "angular-cli": "^1.0.0-beta.10",
    "codelyzer": "^0.0.20",
    "concurrently": "2.2.0",
    "cpx": "1.3.1",
    "http-server": "^0.8.5",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jshint-stylish": "2.2.0",
    "karma": "1.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "^0.0.4",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "karma-spec-reporter": "0.0.26",
    "node-sass": "3.8.0",
    "path": "0.12.7",
    "rimraf": "2.5.4",
    "tslint": "^3.6.0",
    "typescript": "^1.8.10",
    "typings": "1.3.1"
  },
  "author": "Rafael",
  "license": "ISC"
}
